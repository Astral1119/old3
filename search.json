---
---

[
  {% for note in site.notes.blog %}
    {

      "title"    : "{{ note.title | strip_html | escape }}",
      "url"      : "{{ note.url }}",
      "content"  : {{ note.content | newline_to_br | strip_newlines | replace: '<br/>', '' | strip | strip_html | strip | jsonify }}

    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
